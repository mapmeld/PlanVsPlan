head
  title About DARPA
  link(href='/stylesheets/slate.min.css', rel='stylesheet', type='text/css')
  script(type='text/javascript', src='/scripts/versus.js')
body
  div.navbar
    .navbar-inner
      .container
        a.brand(href="#") Plan vs Plan
        div.nav-collapse
          ul.nav
            li
              a(href='/') Showdown
            li
              a(href='/ranking') Ranking
            li.active
              a(href='/d') Add DARPA Item
            li
              a(href='/b') Add Batman Item

  div.container
    div.hero-unit(style='padding:20px;')
      h1 DARPA vs. Batman: DARPA
    
    div.row(style='text-align:center;')
      .span12
        h3 DARPA is the research arm of the Department of Defense.
        div(style='border:1px solid #fff;padding:15px;')
          h4 Unaffiliated
          p
            span PlanVsPlan and this page are&nbsp;
            a(href="http://imgur.com/Zqo8k") NOT affiliated
            span &nbsp;with DARPA, Batman, or DC Comics.
        br
        h3 Submit an UNCLASSIFIED DARPA Gadget
        form.well(action="/submitd", method="GET")
          label.info Item Name
          input.x-large(name='item')
          br
          br
          label.info Photo URL
          input.x-large(name='url')
          hr
          input.btn.btn-primary(type="submit")